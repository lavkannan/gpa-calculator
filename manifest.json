{
  "manifest_version": 2,

  "name": "GPA Calculator",
  "description": "This extension calculates GPA from input grades.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "calc_icon.png",
    "default_popup": "calc.html",
    "default_title": "Calculate your GPA!"
  },
  "permissions": [
    "activeTab"
  ]
}